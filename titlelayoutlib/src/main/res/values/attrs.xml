        <attr name="d_have_line" />

        <attr name="d_is_back_view" />
        <attr name="d_is_immersive_state_bar" />

    </declare-styleable>


    <attr name="d_left_image" format="reference" />
    <attr name="d_left_image_width" format="dimension|reference" />
    <attr name="d_left_image_padding_start" format="dimension|reference" />
    <attr name="d_left_image_padding_end" format="dimension|reference" />

    <attr name="d_left_text" format="reference|string" />
