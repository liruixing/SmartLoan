    <attr name="d_right_image_padding_start" format="dimension|reference" />

    <attr name="d_line_height" format="dimension|reference" />
    <attr name="d_line_background" format="color|reference" />
    <attr name="d_have_line" format="boolean" />

    <attr name="d_title_layout_height" format="dimension|reference" />
    <attr name="d_title_layout_background" format="color|reference" />

    <attr name="d_is_back_view" format="boolean" />
    <attr name="d_is_immersive_state_bar" format="boolean" />

</resources>