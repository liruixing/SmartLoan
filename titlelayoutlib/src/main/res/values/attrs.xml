        <enum name="normal" value="0" />
        <enum name="bold" value="1" />
        <enum name="italic" value="2" />
    </attr>

    <attr name="d_subtitle_text" format="reference|string" />
    <attr name="d_subtitle_size" format="dimension|reference" />
    <attr name="d_subtitle_color" format="dimension|reference" />
    <attr name="d_subtitle_style" format="reference">
        <enum name="normal" value="0" />
        <enum name="bold" value="1" />
        <enum name="italic" value="2" />
    </attr>

    <attr name="d_right_text" format="reference|string" />
    <attr name="d_right_text_color" format="color|reference" />
    <attr name="d_right_text_size" format="dimension|reference" />
    <attr name="d_right_text_style" format="reference">
        <enum name="normal" value="0" />
        <enum name="bold" value="1" />
        <enum name="italic" value="2" />
    </attr>
    <attr name="d_right_text_padding_end" format="dimension|reference" />
